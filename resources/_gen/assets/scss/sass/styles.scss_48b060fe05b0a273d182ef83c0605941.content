.navbar{background:#fff;position:absolute;width:100%;border-bottom:1px solid #ccc;top:0;border-top:4px solid #b81919}.navbar .site-title{font-family:Merriweather,serif;font-weight:700;color:#b81919;font-size:18px;float:left;line-height:60px;vertical-align:middle;text-decoration:none}.side-nav{width:200px;float:left}.side-nav .nav-heading{font-size:14px;font-weight:700;margin-left:8px;padding:8px 0}.side-nav ul{list-style-type:none;padding:0;margin-top:0;margin-bottom:24px}.side-nav li{font-size:14px}.side-nav li a{display:block;padding:8px 16px;border-radius:2px;text-decoration:none;color:#222}.side-nav li a.active{background:#fff;padding-left:12px;border-left:4px solid #b81919}body,button,input,select,label{font-family:open sans,sans-serif;font-size:14px}body{color:#222}h1{border-bottom:1px solid #ccc;padding:8px 0;margin-top:0}h2,h3,h4,h5,h6{font-weight:600}h2{margin:24px 0 2px}a{color:#076cab}p{color:#444}blockquote{color:#777;border-left:4px solid #777;margin:32px 0;padding:0 16px}.content{background:#fff;border:1px solid #ccc;border-radius:2px}.content{float:left;width:calc(100% - 218px);margin-left:16px}.content .content-body{padding:32px}.file-download{background:#eee;display:inline-block;height:54px;border:1px solid #ccc;border-radius:2px;margin-top:12px;margin-right:12px}.file-download img,.file-download .download-title{float:left;padding:18px}.file-download img{height:18px}.file-download .download-title{padding-left:0}.file-download:hover{background:#e1e1e1;border-color:#bfbfbf;box-shadow:0 1px 1px 0 rgba(119,119,119,.3)}body{background:#eee;margin:0;padding-top:calc(64px + 24px);margin-bottom:24px}.container{max-width:1000px;width:calc(100% - 32px);margin:0 auto}.clearfix:after,.file-download:after{content:"";display:table;clear:both}