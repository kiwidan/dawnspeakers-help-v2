.navbar{background:#fff;position:relative;z-index:200;width:100%;border-bottom:1px solid #ccc;top:0;border-top:4px solid #b81919}.navbar .site-title{font-family:Merriweather,serif;font-weight:700;color:#b81919;font-size:18px;float:left;line-height:60px;vertical-align:middle;text-decoration:none}.navbar .navbar-right{float:right}.side-nav{width:200px;float:left}@media(max-width:899px){.side-nav{display:none}}.side-nav .nav-heading{font-size:14px;font-weight:700;margin-left:8px;padding:8px 0}.side-nav ul{list-style-type:none;padding:0;margin-top:0;margin-bottom:24px}.side-nav li{font-size:14px}.side-nav li a{display:block;padding:8px 16px;border-radius:2px;text-decoration:none;color:#222}.side-nav li a.active{background:#fff;padding-left:12px;border-left:4px solid #b81919}body,button,input,select,label{font-family:open sans,sans-serif;font-size:16px}body{color:#222}h1{border-bottom:1px solid #ccc;padding:8px 0;margin-top:0}h2,h3,h4,h5,h6{font-weight:600}h2{margin:24px 0 2px}a{color:#076cab}p{color:#444}blockquote{color:#777;border-left:4px solid #777;margin:32px 0;padding:0 16px}.content{background:#fff;border:1px solid #ccc;border-radius:2px}.content{float:left;width:calc(100% - 218px);margin-left:16px}@media(max-width:899px){.content{width:100%;margin-left:0;border:none}}.content .content-body{padding:32px}.content .content-body>img{width:100%}.file-download{background:#eee;display:inline-block;height:54px;border:1px solid #ccc;border-radius:2px;margin-top:12px;margin-right:12px}.file-download img,.file-download .download-title{float:left}.file-download img{height:18px;padding:18px}.file-download .download-title{padding:16px 18px;padding-left:0;text-decoration:underline}.file-download:hover{background:#e1e1e1;border-color:#bfbfbf;box-shadow:0 1px 1px 0 rgba(119,119,119,.3)}.mobile-nav-button{position:relative;z-index:100;width:24px;height:17px;transform:rotate(0deg);transition:.5s ease-in-out;cursor:pointer;margin:21px 0;display:none}@media(max-width:899px){.mobile-nav-button{display:block}}.mobile-nav-button span{display:block;position:absolute;height:3px;width:100%;background:#b81919;border-radius:5px;opacity:1;left:0;transform:rotate(0deg);transition:.25s ease-in-out}.mobile-nav-button span:nth-child(1){top:0}.mobile-nav-button span:nth-child(2){top:7px}.mobile-nav-button span:nth-child(3){top:7px}.mobile-nav-button span:nth-child(4){top:14px}.mobile-nav-button.open span:nth-child(1),.mobile-nav-button.open span:nth-child(4){top:14px;width:0%;left:50%}.mobile-nav-button.open span:nth-child(2){transform:rotate(45deg)}.mobile-nav-button.open span:nth-child(3){transform:rotate(-45deg)}.mobile-menu-slide-wrapper{max-height:0;overflow:hidden;z-index:100;position:relative;transition:max-height .5s ease-in-out}@media(max-width:899px){.mobile-menu-slide-wrapper.open{border-bottom:1px solid #ccc;max-height:10000px}}.mobile-menu{background:#fff;padding:8px 0 24px}.mobile-menu .nav-heading,.mobile-menu .menu-item{padding:8px 32px}.mobile-menu .nav-heading{font-weight:700;padding-bottom:8px;padding-top:24px}.mobile-menu-page-dimmer{display:none;position:fixed;background:rgba(0,0,0,.5);transition:background .5s ease-in-out;top:0;bottom:0;left:0;right:0}.mobile-menu-open .mobile-menu-page-dimmer{display:block}body{background:#eee;margin:0;margin-bottom:24px}@media(max-width:899px){body{margin-bottom:0}}.container{max-width:1000px;width:calc(100% - 32px);margin:0 auto}@media(max-width:899px){.container.mobile__full-width{width:100%}}.clearfix:after,.file-download:after{content:"";display:table;clear:both}